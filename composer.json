{
    "name": "teqneers/ext-application-bundle",
    "type": "library",
    "description": "A Symfony bundle to integrate Sencha Ext JS into a Symfony application",
    "homepage": "https://github.com/teqneers/ext-application-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "http://opensource.teqneers.de/"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\Bundle\\ExtJSApplicationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\Bundle\\ExtJSApplicationBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "symfony/config": "~3.0 || ~4.0 || ~5.0",
        "symfony/dependency-injection": "~3.0 || ~4.0 || ~5.0",
        "symfony/http-kernel": "~3.0 || ~4.0 || ~5.0",
        "symfony/routing": "~3.0 || ~4.0 || ~5.0",
        "symfony/yaml": "~3.0 || ~4.0 || ~5.0",
        "teqneers/ext-application": "~2.0",
        "twig/twig": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "~3.0 || ~4.0 || ~5.0",
        "symfony/phpunit-bridge": "~5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
